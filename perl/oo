package Some::Module::oo;

use strict;
use warnings;

our $VERSION = 0.00;

sub new {
        my($class) = shift;

        my($self) = {};
        bless $self, $class;

	$self->_init(@_);
        return $self;
}

sub _init {
	my($self) = shift;

	return $self;
}


END { }       # module clean-up code here (global destructor)

1;  # don't forget to return a true value from the file

__END__
